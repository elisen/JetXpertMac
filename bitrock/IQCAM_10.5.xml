    <createUninstaller>0</createUninstaller>
    <defaultUnixGroup>staff</defaultUnixGroup>
    <defaultUnixOwner>${system_username}</defaultUnixOwner>
    <enableEncryption>1</enableEncryption>
    <enableRollback>0</enableRollback>
    <enableTimestamp>1</enableTimestamp>
    <encryptionPassword>IQ123</encryptionPassword>
    <osxPlatforms>osx-intel osx-x86_64</osxPlatforms>
    <osxSigningIdentity>Developer ID Application: ImageXpert, Inc. (L4HA83SRMM)</osxSigningIdentity>
    <requireInstallationByRootUser>1</requireInstallationByRootUser>
    <vendor>ImageXpert inc.</vendor>
    <parameterList>
        <directoryParameter>
            <name>installdir</name>
            <description>Installer.Parameter.installdir.description</description>
            <explanation>Installer.Parameter.installdir.explanation</explanation>
            <value></value>
            <default>/Applications/ImageXpert 10.5</default>
            <allowEmptyValue>0</allowEmptyValue>
            <ask>0</ask>
            <cliOptionName>prefix</cliOptionName>
            <mustBeWritable>1</mustBeWritable>
            <mustExist>0</mustExist>
            <width>30</width>
        </directoryParameter>
        <choiceParameter>
            <name>preferred_env</name>
            <title>Choose Image Quality configuration</title>
            <description></description>
            <explanation>Choose the Image Quality installation based on your current configuration </explanation>
            <value></value>
            <default>2cams</default>
            <allowEmptyValue>0</allowEmptyValue>
            <displayType>radiobuttons</displayType>
            <insertAfter>installdir</insertAfter>
            <ordering>default</ordering>
            <width>30</width>
            <optionList>
                <option>
                    <description>Installs Image Quality Camera on a JetXpert system</description>
                    <image></image>
                    <text>config: total 2 cameras</text>
                    <value>2cams</value>
                </option>
                <option>
                    <description>Installs Image Quality Camera on a JetXpert system with wetting or nozzleXaminer OR on JetXpert 3D.</description>
                    <image></image>
                    <text>config: total 3 cameras</text>
                    <value>3cams</value>
                </option>
                <option>
                    <description>Installs Image Quality Camera on a JetXpert system with wetting or on JetXpert 3D and NozzleXaminer.</description>
                    <image></image>
                    <text>config: total 4 cameras</text>
                    <value>4cams</value>
                </option>
            </optionList>
            <postShowPageActionList>
                <componentSelection>
                    <deselect>4cam_config,3cam_config</deselect>
                    <select>2cam_config</select>
                    <ruleList>
                        <compareValues>
                            <logic>equals</logic>
                            <value1>${preferred_env}</value1>
                            <value2>2cams</value2>
                        </compareValues>
                    </ruleList>
                </componentSelection>
                <componentSelection>
                    <deselect>4cam_config,2cam_config</deselect>
                    <select>3cam_config</select>
                    <ruleList>
                        <compareValues>
                            <logic>equals</logic>
                            <value1>${preferred_env}</value1>
                            <value2>3cams</value2>
                        </compareValues>
                    </ruleList>
                </componentSelection>
                <componentSelection>
                    <deselect>2cam_config,3cam_config</deselect>
                    <select>4cam_config</select>
                    <ruleList>
                        <compareValues>
                            <logic>equals</logic>
                            <value1>${preferred_env}</value1>
                            <value2>4cams</value2>
                        </compareValues>
                    </ruleList>
                </componentSelection>
            </postShowPageActionList>
        </choiceParameter>
    </parameterList>
</project>

